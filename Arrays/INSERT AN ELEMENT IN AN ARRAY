def main():
    arr = [1, 20, 5, 78, 30]
    size = len(arr)
    
    pos = int(input("Enter position: "))
    element = int(input("Enter element: "))

    if 0 <= pos <= size:
        arr.insert(pos, element)
        for i in range(len(arr)):
            print(arr[i], end=" ")
    else:
        print("Invalid Position")
if __name__ == "__main__":
    main()
