def maxArea(height):
    
    # Write your code here.
    n=len(height)
    i=0
    j=n-1
    maxWater=0
    while i<j:
        w=j-i
        h=min(height[i],height[j])
        area=w*h 
        maxWater=max(maxWater,area)
        if height[i]>height[j]:
            j-=1
        else:
            i+=1
    return maxWater
    pass
