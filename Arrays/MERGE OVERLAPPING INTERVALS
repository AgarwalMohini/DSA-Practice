class Solution:
    def merge(self, intervals: List[List[int]]) -> List[List[int]]:
        n=len(intervals)
        intervals.sort()
        ans=[]
        i=0
        while i<n:
            start=intervals[i][0]
            end=intervals[i][1]
            j=i+1
            while j<n and intervals[j][0]<=end:
                end=max(end,intervals[j][1])
                j+=1
            ans.append([start,end])
            i=j
        return ans
