from os import *
from sys import *
from collections import *
from math import *

def floorSearch(arr, x, n):
	
	# Write your code here.
	start=0
	end=len(arr)-1
	res=-1
	while start<=end:
		mid=(start+end)//2
		if x==arr[mid]:
			return arr[mid]
		if arr[mid]<x:
			res=arr[mid]
			start=mid+1
		elif arr[mid]>x:
			end=mid-1
	return res

	pass
